<tr data-id="<%= gds_uid %>">
    <td><%= gds_uid %></td>
    <td><input type="text" name="title" value="<%= title %>" data-id="<%= gds_uid %>" /></td>
    <td><input type="text" name="units" value="<%= units %>" data-id="<%= gds_uid %>" /></td>
    <td><input type="text" name="quantity" value="<%= quantity %>" data-id="<%= gds_uid %>" /></td>
    <td><input type="text" name="price" value="<%= price %>" data-id="<%= gds_uid %>" /></td>
    <td><input type="button" name="remove" value="x" data-id="<%= gds_uid %>" /></td>
</tr>
